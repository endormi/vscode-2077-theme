      "settings": {
        "fontStyle": "",
        "foreground": "#ff06ee"
      }
    },
    {
      "name": "Meta",
      "scope": "meta",
      "settings": {
        "foreground": "#0ef3ff"
      }
    },
    {
      "name": "Meta JSX",
      "scope": [
        "meta.jsx.children",
        "meta.jsx.children.js",
        "meta.jsx.children.tsx"
      ],
      "settings": {
        "foreground": "#0ef3ff"
      }
    },
    {
      "name": "Punctuation Parameters",
